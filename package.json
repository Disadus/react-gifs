{
  "name": "gif-player",
  "version": "0.0.0",
  "description": "react gif player",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "size": "npm run build && size-limit",
    "build": "rollup -c",
    "prepare": "npm run build",
    "start-demo": "parcel demo/index.html --out-dir demo/dist --open",
    "build-demo": "rm -rf 'demo/dist/' && parcel build demo/index.html --out-dir demo/dist --public-url https://jeetiss.github.io/gif-player/",
    "deploy-demo": "npm run build-demo && gh-pages -d demo/dist"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "name": "useLoader",
      "import": "{ useLoader }",
      "limit": "5 KB"
    },
    {
      "path": "dist/index.js",
      "name": "useWorkerLoader",
      "import": "{ useWorkerLoader }",
      "limit": "5 KB"
    },
    {
      "path": "dist/index.js",
      "name": "usePlayerState",
      "import": "{ usePlayerState }",
      "limit": "5 KB"
    },
    {
      "path": "dist/index.js",
      "name": "useMotor",
      "import": "{ useMotor }",
      "limit": "5 KB"
    },
    {
      "path": "dist/index.js",
      "name": "Canvas",
      "import": "{ Canvas }",
      "limit": "5 KB"
    }
  ],
  "sideEffects": false,
  "dependencies": {
    "gifuct-js": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.13",
    "react-dom": ">=16.13"
  },
  "keywords": [],
  "author": "jeetiss",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^4.9.2"
  }
}
